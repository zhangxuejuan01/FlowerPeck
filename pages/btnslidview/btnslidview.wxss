page{
  background: #fff;
}
.clearFix:after{
    display: block;
    content:'';
    clear: both;
    height:0;
    }
.header{
  height: 80rpx;
  width: 750rpx;
  line-height: 80rpx;
  padding-left: 10rpx;
  padding-top: 20rpx;
  box-sizing: border-box;
  border: 1rpx solid #f1f1f1;
}
.txt{
  display: block;
  height: 35rpx;
  line-height: 35rpx;
  color: #1E90FF;
  font-size: 28rpx;
  font-weight: bold;
  border-top-left-radius: 3rpx;
  border-bottom-left-radius: 3rpx;
  border-left: 8rpx solid #1E90FF;
  padding-left: 12rpx;
  box-sizing: border-box;
}
.iitem{
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  color: #666;
  padding-left: 30rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f1f1f1;
}
.iitem text{
  float: right;
}